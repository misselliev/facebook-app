<%if liked?(locals[:post])%>
  <%= link_to create_like_path(locals[:post], from: locals[:from]), method: :post do %>
    <i class="heart grey like icon"></i>
  <% end %>
<%else%>
  <%= link_to destroy_like_path(locals[:post], from: locals[:from]), method: :delete do %>
    <i class="heart teal like icon"></i>
  <% end %>
<%end%>